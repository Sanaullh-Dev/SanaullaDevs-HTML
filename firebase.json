{
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [{
      "source": "index.html",
      "destination": "/index.html"
    },
    {
      "source": "css/style.css",
      "destination": "/css/style.css"
    }]
  }
}
